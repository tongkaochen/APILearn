<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tifone.ui">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".recyclerview.MyRecyclerViewActivity" >
        </activity>
        <activity android:name=".coordinator.CoordinatorActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"> </activity>
        <activity android:name=".surfaceview.ViewActivity"
            android:theme="@style/Widget.AppCompat.ActionBar"/>
        <activity android:name=".surfaceview.SurfaceViewActivity"
            android:theme="@style/Widget.AppCompat.ActionBar"/>
        <activity android:name=".coordinator.MdStyleActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar"/>
        <activity android:name=".animation.AnimationActivity" />
        <activity android:name=".calculator.CalculatorActivity"
            android:theme="@style/AppTheme.Coordinator"/>
        <activity android:name=".json.JsonActivity" />
        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity android:name=".okhttp.OkhttpActivity" />
        <activity android:name=".rxjava.RxActivity" />
        <activity android:name=".customview.CustomViewActivity" />
        <activity android:name=".xmlparser.XmlParserActivity" />
        <activity android:name=".notification.NotificationActivity" />
        <activity android:name=".retrofit.RetrofitActivity" />
        <activity android:name=".annotation.AnnotationActivity" />
        <activity android:name=".glide.GlideActivity" />
    </application>

</manifest>